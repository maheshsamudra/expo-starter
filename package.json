{
  "name": "expo-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build-a": "eas build --profile staging --platform android",
    "build-i": "eas build --profile staging --platform ios",
    "build-a-production": "eas build --profile production --platform android",
    "build-i-production": "eas build --profile production --platform ios",
    "build-dev-a": "eas build --profile development --platform android",
    "build-dev-i": "eas build --profile development --platform ios",
    "dev": "npx expo start --dev-client"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "expo": "~48.0.18",
    "expo-constants": "~14.2.1",
    "expo-linking": "~4.0.1",
    "expo-notifications": "~0.18.1",
    "expo-router": "^1.0.0",
    "expo-status-bar": "~1.4.4",
    "firebase": "^10.0.0",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "expo-dev-client": "~2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "prettier": "^3.0.0"
  },
  "private": true
}
